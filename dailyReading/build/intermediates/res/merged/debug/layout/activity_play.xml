                <ImageView
                    style="@style/ww_parent"
                    android:src="@drawable/star_icon_1_09_she_shou_normal" />

                <TextView
                    style="@style/Constellation_Date_Tv_Styles"
                    android:text="@string/sheshou_date" />

                <TextView
                    style="@style/Constellation_Title_Tv_Styles"
                    android:text="@string/sheshou" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/mojie"
                style="@style/Constellation_Item_Styles" >

                <ImageView
                    style="@style/ww_parent"
                    android:src="@drawable/star_icon_1_10_mo_jie_normal" />

                <TextView
                    style="@style/Constellation_Date_Tv_Styles"
                    android:text="@string/mojie_date" />


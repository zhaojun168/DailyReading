                <TextView
                    style="@style/Constellation_Date_Tv_Styles"
                    android:text="@string/shuiping_date" />

                <TextView
                    style="@style/Constellation_Title_Tv_Styles"
                    android:text="@string/shuiping" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/shuangyu"
                style="@style/Constellation_Item_Styles" >

                <ImageView
                    style="@style/ww_parent"
                    android:src="@drawable/star_icon_1_12_shuang_yu_normal" />

                <TextView
                    style="@style/Constellation_Date_Tv_Styles"
                    android:text="@string/shuangyu_date" />

                <TextView
                    style="@style/Constellation_Title_Tv_Styles"
                    android:text="@string/shuangyu" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>